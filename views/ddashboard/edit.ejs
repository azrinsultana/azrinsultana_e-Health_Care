                                        <input class="upload" type="file">
                                    </div>
                                </div>
                                <% if (typeof evalue != 'undefined') { %>
                                    <% evalue.forEach(function(std) { %>
                            
                            
                                       <h3 style="color: crimson;"><%= std.msg %></h3> 
                                       <% })%>
                            
                                       <% } %>

                                <div class="profile-basic">
                                    <div class="row">
                                        <div class="col-md-6">
                                           

                                            <div class="form-group form-focus">
                                                <label class="focus-label">User Name:
                                                <%=doctor[0].username%></label>
                                                
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <label class="focus-label">Name:</label>
                                            <div class="form-group ">
                                               
