                                            <label class="focus-label">Password:</label>
                                            <div class="form-group ">
                                                
                                              
                                                    <input class="form-control floating " type="text" name="password" value="<%=doctor[0].password%>">
                                                
                                            </div>
                                        </div>
                                       
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card-box">
